   "source": [
    "equation=[]\n",
    "from sympy.solvers import solve\n",
    "from sympy import symbols\n",
    "while 1:\n",
    "    a=input(\"請輸入方程式:\")\n",
    "    if a==\"\":\n",
    "        break\n",
    "    arr=a.split(\"=\")\n",
    "    if len(arr)==2:\n",
    "        a=arr[0]+\"-(\"+arr[1]+\")\"\n",
    "        equation.append(a)\n",
    "print(equation)\n",
    "solve(equation) "
